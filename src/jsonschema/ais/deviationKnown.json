{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "http://127.0.0.1:8000/ais/deviationKnown",
  "title": "deviationKnown",
  "description": "The VSOC also receives information from the AIS. For this, the function ais_deviation_known() to get information from the AIS for an known deviation. The request is a POST request from the AIS to the VSOC.",
  "type": "object",
  "properties": {
    "relationships": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "type": {
            "description": "property extension from the request (coming from the STIX format)",
            "const": "relationship"
          },
          "spec_version": {
            "description": "version of the stix format",
            "type": "string"
          },
          "id": {
            "type": "string",
            "pattern": "^relationship--.*$"
          },
          "created": {
            "description": "timestamp of the creation in ISO-8601 (UTC)",
            "type": "string",
            "format": "date-time"
          },
          "modified": {
            "description": "timestamp of the modification in ISO-8601 (UTC)",
            "type": "string",
            "format": "date-time"
          },
          "relationship_type": {
            "description": "relationship type of the deviation",
            "type": "string"
          },
          "source_ref": {
            "description": "reference to the source indicator",
            "type": "string",
            "pattern": "^indicator--.*$"
          },
          "target_ref": {
            "description": "reference to the target indicator",
            "type": "string",
            "pattern": "^indicator--.*$"
          }
        },
        "required": [
          "type",
          "spec_version",
          "id",
          "created",
          "modified",
          "relationship_type",
          "source_ref",
          "target_ref"
        ],
        "additionalProperties": false
      }
    },
    "indicator": {
      "type": "object",
      "properties": {
        "type": {
          "description": "property extension from the request (coming from the STIX format)",
          "const": "indicator"
        },
        "spec_version": {
          "description": "version of the stix format",
          "type": "string"
        },
        "id": {
          "type": "string",
          "pattern": "^indicator--.*$"
        },
        "created": {
          "description": "timestamp of the creation in ISO-8601 (UTC)",
          "type": "string",
          "format": "date-time"
        },
        "modified": {
          "description": "timestamp of the modification in ISO-8601 (UTC)",
          "type": "string",
          "format": "date-time"
        },
        "name": {
          "const": "Anomaly detection"
        },
        "description": {
          "const": "An immunological algorithm detected a deviation in real-time dataset."
        },
        "indicator_types": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "minItems": 1,
          "uniqueItems": true
        },
        "pattern": {
          "description": "A specific pattern relevant to the reported case",
          "type": "string"
        },
        "pattern_type": {
          "const": "stix"
        },
        "pattern_version": {
          "const": "2.1"
        },
        "valid_from": {
          "description": "timestamp of the start of the validity period in ISO-8601 (UTC)",
          "type": "string",
          "format": "date-time"
        },
        "valid_until": {
          "description": "timestamp of the end of the validity period in ISO-8601 (UTC)",
          "type": "string",
          "format": "date-time"
        },
        "labels": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "minItems": 1,
          "uniqueItems": true
        },
        "extensions": {
          "type": "object",
          "patternProperties": {
            "^extension-definition--.*$": {
              "type": "object",
              "properties": {
                "extension_type": {
                  "type": "string"
                }
              },
              "required": [
                "extension_type"
              ],
              "additionalProperties": false
            }
          },
          "additionalProperties": false,
          "minProperties": 1
        },
        "source_vehicle": {
          "description": "VIN of the source vehicle",
          "type": "string"
        },
        "source_ais": {
          "description": "ID of the source AIS",
          "type": "string"
        },
        "source_rsu": {
          "description": "ID of the source RSU",
          "type": "string"
        },
        "src_ref": {
          "description": "IP address of the source",
          "type": "string"
        },
        "dst_ref": {
          "description": "IP address of the destination",
          "type": "string"
        },
        "src_port": {
          "description": "Port of the source",
          "type": "string"
        },
        "dst_port": {
          "description": "Port of the destination",
          "type": "string"
        },
        "protocol_type": {
          "type": "string"
        },
        "service": {
          "type": "string"
        },
        "flag": {
          "type": "string"
        },
        "timestamp": {
            "description": "timestamp of the from the AIS endpoint ISO-8601 (UTC)",
            "type": "string",
            "format": "date-time"
        }
      },
      "required": [
        "type",
        "spec_version",
        "id",
        "created",
        "modified",
        "name",
        "description",
        "indicator_types",
        "pattern",
        "pattern_type",
        "pattern_version",
        "valid_from",
        "valid_until",
        "labels",
        "extensions",
        "source_vehicle",
        "source_ais",
        "source_rsu",
        "src_ref",
        "dst_ref",
        "src_port",
        "dst_port",
        "protocol_type",
        "service",
        "flag",
        "timestamp"
      ],
      "additionalProperties": false
    }
  },
  "required": [
    "relationships",
    "indicator"
  ],
  "additionalProperties": false
}
